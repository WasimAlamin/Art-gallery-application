@model LaborationRep.Models.Artwork

@{
    ViewData["Title"] = "Details";
}

<h1>Detaljer</h1>

<div>
    <h4>@Html.DisplayFor(m => m.ArtworkName) </h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Konstverk
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ArtworkName)
        </dd>
        <dt class = "col-sm-2">
            År
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Year)
        </dd>
        <dt class = "col-sm-2">
            Genre
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Genre)
        </dd>
        <dt class = "col-sm-2">
            Teknik
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Technique)
        </dd>
        <dt class = "col-sm-2">
            Rum
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Room)
        </dd>
        <dt class = "col-sm-2">
            Plats
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Location)
        </dd>
        <dt class = "col-sm-2">
            Utställning
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Exhibition)
        </dd>
        <dt class = "col-sm-2">
            Konstnär
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Artist.FirstName) @Html.DisplayFor(model => model.Artist.LastName)
        </dd>
    </dl>
</div>
<div>
            <a asp-action="Edit" asp-route-id="@Model.Id">Uppdatera</a> <br/> |
            @Html.ActionLink("Tillbaka till konstverklista", "Index", new { id = Model.ArtistId })

</div>
